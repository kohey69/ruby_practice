a = Rational(2, 5)
b = Rational(1, 3)
p [a, b]

c = a + b
p c
p c.to_f
p [c.numerator, c.denominator]

c = Complex(1, 2)
p c
p [c.real, c.imaginary]

p 1 + 1 #=> 2
p 1 + 1.0 #=> 2.0
p 2 - 1 #=> 1
p 2 - 1.0 #=> 1.0
p 3 * 2 #=> 6
p 3 * 2.0 #=> 6.0
p 3 * -2.0 #=> -6.0
p 5 / 2 #=> 2
p 5 / 2.0 #=> 2.5
p 5 % 2 #=> 1
p 5 % 2.0 #=> 1.0
p 5 ** 2 #=> 25
p 5 ** 0.5 #=> 2.23606797749979

r = (2 / 5r) + (1 / 3r)
p r #=> (11/15)
p r.to_f #=> 0.7333333333333333
c = (1 + 2i) ** 2
p c #=> (-3+4i)

p 5 ** -2.0 #=> 0.04
p 5 ** -2 #=> (1/25)
